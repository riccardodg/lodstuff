<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY foaf "http://xmlns.com/foaf/0.1" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl" >
    <!ENTITY gn "http://www.geonames.org/ontology" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns" >
    <!ENTITY auth "http://www.resourcebook.eu/lremap/owl/lremap_auth" >
    <!ENTITY base "http://www.resourcebook.eu/lremap/owl" >
    


<rdf:RDF xmlns="&base;"
     xmlns:gn="&gn;#"
     xmlns:rdfs="&rdfs;#"
     
     xmlns:foaf="&foaf;#"
     xmlns:auth="&auth;"
     xmlns:owl="&owl;#"
     xmlns:xsd="&xsd;#"
     xmlns:rdf="&rdf;#">
    <owl:Ontology rdf:about="&auth;">
        <rdfs:comment>
        	This is the simplest ontology covering the basic author information of the lremap. It imports http://www.resourcebook.eu/lremap/owl/obj_auth.owl which contains the basic Person, affiliation and Geographical information. It contains instances extracted from the lremap db.
    	</rdfs:comment>
    </owl:Ontology>
