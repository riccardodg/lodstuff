<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns" >
    <!ENTITY year "http://www.resourcebook.eu/lremap/owl/lremap_year" >
    <!ENTITY conf "http://www.resourcebook.eu/lremap/owl/lremap_conf" >
    <!ENTITY base "http://www.resourcebook.eu/lremap/owl" >
    <!ENTITY swc "http://data.semanticweb.org/ns/swc/ontology" >
    <!ENTITY tl "http://purl.org/NET/c4dm/timeline.owl" >
    <!ENTITY geo "http://www.w3.org/2003/01/geo/wgs84_pos" >
]>


<rdf:RDF xmlns="&conf;#"
     xmlns:dc="&dc;"
     xmlns:rdfs="&rdfs;#"
     xmlns:swc="&swc;#"
     xmlns:tl="&tl;#"
     xmlns:geo="&geo;#"
     xmlns:owl="&owl;#"
     xmlns:xsd="&xsd;#"
     xmlns:rdf="&rdf;#"
     xmlns:conf="&conf;"
     xmlns:year="&year;">
    <owl:Ontology rdf:about="&conf;">
        <rdfs:label rdf:datatype="&xsd;string">Modeled on http://data.semanticweb.org/ns/swc/ontology</rdfs:label>
    </owl:Ontology>
   
