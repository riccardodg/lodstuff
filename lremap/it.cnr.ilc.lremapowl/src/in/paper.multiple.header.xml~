    <!ENTITY auth "http://www.resourcebook.eu/lremap/owl/instances/" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/">
    <!ENTITY base "http://www.resourcebook.eu/lremap/owl" >
]>


