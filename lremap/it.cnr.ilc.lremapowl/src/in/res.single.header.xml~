<!DOCTYPE rdf:RDF [
    <!ENTITY bibo "http://purl.org/ontology/bibo/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns" >
    <!ENTITY auth "http://www.resourcebook.eu/lremap/owl/lremap_auth" >
    <!ENTITY paper "http://www.resourcebook.eu/lremap/owl/lremap_paper" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/">
    <!ENTITY base "http://www.resourcebook.eu/lremap/owl/" >
]>


<rdf:RDF xmlns="&paper;"
     xmlns:rdfs="&rdfs;#"
     xmlns:auth="&auth;"
     xmlns:owl="&owl;#"
     xmlns:xsd="&xsd;#"
     xmlns:rdf="&rdf;#"
     xmlns:paper="&paper;"
     xmlns:dc="&dc;"
     xmlns:bibo="&bibo;">
     <owl:Ontology rdf:about="http://www.resourcebook.eu/lremap/owl/lremap_ri.owl">
        <rdfs:comment>This is the collection of resource instances. It imports the Resource and the submissions.</rdfs:comment>
        <owl:imports rdf:resource="http://lexvo.org/ontology"/>
        <owl:imports rdf:resource="http://www.resourcebook.eu/lremap/owl/lremap_resource.owl"/>
    </owl:Ontology>
    


<!DOCTYPE rdf:RDF [
    <!ENTITY lvont "http://lexvo.org/ontology" >
    <!ENTITY terms "http://purl.org/dc/terms/" >
    <!ENTITY dcterms "http://purl.org/dc/terms/#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema##" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY ri "http://www.resourcebook.eu/lremap/owl/lremap_ri.owl#" >
    <!ENTITY lremap "http://www.resourcebook.eu/lremap/owl/lremap_resource.owl#" >
]>


<rdf:RDF xmlns="http://www.resourcebook.eu/lremap/owl/lremap_ri.owl"
     xml:base="http://www.resourcebook.eu/lremap/owl/lremap_ri.owl"
     xmlns:lremap="http://www.resourcebook.eu/lremap/owl/lremap_resource.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:lvont="http://lexvo.org/ontology"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:ri="http://www.resourcebook.eu/lremap/owl/lremap_ri.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="&xsd;#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:dcterms="&terms;#">
    <owl:Ontology rdf:about="http://www.resourcebook.eu/lremap/owl/lremap_ri.owl">
        <rdfs:comment>This is the collection of resource instances. It imports the Resource and the submissions.</rdfs:comment>
        <owl:imports rdf:resource="http://lexvo.org/ontology"/>
        <owl:imports rdf:resource="http://www.resourcebook.eu/lremap/owl/lremap_resource.owl"/>
    </owl:Ontology>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.resourcebook.eu/lremap/owl/lremap_paper#hasKeyword -->

    <owl:DatatypeProperty rdf:about="&paper;#hasKeyword">
        <rdfs:domain rdf:resource="&bibo;Document"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    

    <!-- http://www.resourcebook.eu/lremap/owl/lremap_paper#status -->

    <owl:Class rdf:about="&paper;#status">
        <rdfs:subClassOf rdf:resource="&bibo;DocumentStatus"/>
    </owl:Class>
